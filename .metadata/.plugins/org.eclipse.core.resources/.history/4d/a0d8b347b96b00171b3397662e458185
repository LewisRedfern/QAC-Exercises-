package Intermediate;
import java.util.*; 

public class Garage {

	private static double price;
	private static HashMap<Integer, String> garageList = new HashMap<Integer, String>();   
	private static int id = 0;  
	
	public static void main(String[] args) {
		
		Car c1 = new Car (5, 5, "Honda", 168.5, 3, true);
		Motorcycle m1 = new Motorcycle (2, 1, "Yamaha", 201.0, 1000, false);
		Boat b1 = new Boat (7, 10, "Sppedster", 50.5, 3, true);
		
		
		
		garageList.put(id++, c1.strMaker());
		garageList.put(id++, m1.strMaker());
		garageList.put(id++, b1.strMaker());

		for (String v : garageList.values()) {
			
			System.out.println(v);
		}
	}
	
	
}
