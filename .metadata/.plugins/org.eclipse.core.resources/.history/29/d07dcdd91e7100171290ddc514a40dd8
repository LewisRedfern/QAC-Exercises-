package JavaSEW2.Practice;

public class TooHot {

	public void run () {
		
		
	}
	
	public void season (int temp) {
		
		int temperature = temp;
		boolean isSummer = false;
		
		if (temperature <= 100 || temperature >= 60) {
			
			isSummer = true;
		}
		
		else if (temperature <=90 || temperature >=60) {
			
			isSummer = true;
		}
		
		else {
			
			isSummer = false;
		}
		
	}
}
