package offlineWork;

public class Task2 {

	String s1 = "breadjambread";
	String s2 = "xxbreadjambreadyy";
	String s3 = "xxbreadyy";

	
	public void run() {
		
		sep(s1);
	}
	
	public void sep (String s) {
		
		String [] sep = s.split("bread");
		
		String a = sep[0];
		String b = sep[1];
		String c = sep[2];
		
		
		System.out.println(a+b+c);
	}
}
